{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1",
      "status": 200,
      "headers": {
        "Content-Type": "text/html; charset=UTF-8"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
